{
  "preset": "./jest.config.json",
  "testMatch": ["<rootDir>/test/e2e/**/*.e2e-spec.ts"],
  "coverageDirectory": "<rootDir>/coverage/e2e",
  "coverageThreshold": {
    "global": {
      "branches": 70,
      "functions": 75,
      "lines": 75,
      "statements": 75
    }
  },
  "testTimeout": 15000,
  "maxConcurrency": 1
}
