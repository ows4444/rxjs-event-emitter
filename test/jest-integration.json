{
  "preset": "./jest.config.json",
  "testMatch": ["<rootDir>/test/integration/**/*.int-spec.ts"],
  "coverageDirectory": "<rootDir>/coverage/integration",
  "coverageThreshold": {
    "global": {
      "branches": 75,
      "functions": 80,
      "lines": 80,
      "statements": 80
    }
  },
  "testTimeout": 10000
}
